{
    "name": "tomrf/nanofraim",
    "type": "project",
    "description": "minimal framework",
    "keywords": [ "framework", "nanofraim" ],
    "homepage": "https://github.com/tomrf/nanofraim",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "psr/container": "^2.0",
        "cache/filesystem-adapter": "^1.2",
        "cache/redis-adapter": "^1.1",
        "cache/array-adapter": "^1.2",
        "cache/simple-cache-bridge": "^1.2",
        "cache/void-adapter": "^1.2",
        "tomrf/autowire": "^0.0.5",
        "tomrf/configcontainer": "^0.0.2",
        "tomrf/servicecontainer": "^0.0.3",
        "tomrf/logger": "^0.0.1",
        "nyholm/psr7-server": "^1.0",
        "nyholm/psr7": "^1.5",
        "relay/relay": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.8",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Nanofraim\\": "src/"
        }
    },
    "scripts": {
        "cs-fixer": [
            "php-cs-fixer fix --allow-risky=yes --diff --verbose"
        ],
        "phpstan": [
            "phpstan analyze --level max src/ app/ bootstrap/ public/"
        ]
    }
}
